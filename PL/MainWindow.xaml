<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Icon="onlineshopping_117952.ico"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <ImageBrush x:Key="AdminIcon" ImageSource="manager__icon.png"  />
    </Window.Resources>
    <Grid>
        <Button HorizontalAlignment="Center" VerticalAlignment="Center" Height="109" Width="252" Click="Button_Click" FontSize="36" FontWeight="Bold" FontFamily="Rod">
            <Border Background="{StaticResource AdminIcon}">
                
            </Border>
        </Button>

    </Grid>
</Window>
